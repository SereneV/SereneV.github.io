<script>
  jQuery(document).ready(function(jQuery){
    
    
	// If region sent via URL update cookie to region defined in url
    var currentURL=document.URL.split('?')[0];
    var regionUrl=document.URL.split('?')[1];
    if(regionUrl == undefined){regionUrl = ''; }

    console.log(regionUrl);
    
    // If region is sent in url
    if(regionUrl != ''){
      if(regionUrl == 'region=AUD'){
        Cookies.remove('locationpopupcookie', { path: '/' });
        Cookies.set('locationpopupcookie', 'AUD', { path: '/', expires: 365 });
        jQuery('.country .country_code').text('Australia');
      }
      else if(regionUrl == 'region=EUR'){
        Cookies.remove('locationpopupcookie', { path: '/' });
        Cookies.set('locationpopupcookie', 'EUR', { path: '/', expires: 365 });
         jQuery('.country .country_code').text('Europe');
      } 
    }   

    // Popup window
   function popupRedirectStore(){
       
           $.magnificPopup.open({
             items: {
               src: '#regionselect',
               type: 'inline'
             },
             mainClass: 'mfp-fade',
             closeOnBgClick: true,
             closeBtnInside: true,
             closeOnContentClick: false,
             // When elemened is focused, some mobile browsers in some cases zoom in
             // It looks not nice, so we disable it:
             callbacks: {
               beforeClose: function() {
                 jQuery('.country .country_code').text(Cookies.get('locationpopupcookie'));
               }
             }   
           });    
  }
           
    function getRegionName(regionCodeVar){
      var regionNameVar = '';
      if(regionCodeVar == 'AUD'){ regionNameVar = 'Australia';}
      else if(regionCodeVar == 'EUR'){ regionNameVar = 'Europe';}
      return regionNameVar;
    }
                                      
    function getRegionURL(regionCodeVar){
      var regionURLVar = '';
      if(regionCodeVar == 'AUD'){ regionURLVar = '://www.thecriticalslidesociety.com';}
      else if(regionCodeVar == 'EUR'){ regionURLVar = '://www.thecriticalslidesociety.eu';}
      return regionURLVar;
    }
    
    // Redirect store to current url
    function redirectStore(countrycode,countryname) {
      jQuery('.country .country_code').text(countryname);
      var currentURL=parent.document.URL.split('?')[0];
      var regionRedirectURL = getRegionURL(countrycode);
      var newCurrentURL = '';
      if('AUD' == 'AUD'){
         if (currentURL.indexOf("/pages/") >= 0){
         var newCurrentURL = 'https' + regionRedirectURL; 
      	}
      	else{
          var newCurrentURL = currentURL.replace('://www.thecriticalslidesociety.com',regionRedirectURL);
        }
      }
      else if('AUD' == 'EUR'){
        var newCurrentURL = currentURL.replace('://www.thecriticalslidesociety.eu',regionRedirectURL); 
      }
      newCurrentURL = newCurrentURL + '?region=' + countrycode;
      console.log(newCurrentURL);
      parent.location.href = newCurrentURL;
    } 
     
    // If select other store in country picker redirect store                                          
    jQuery('a.country_wrapper').click(function(e) { 
      e.preventDefault(); 
      popupRedirectStore();
    });
      
    // If select other store in country picker redirect store                                          
    jQuery('.store-select.newstore').click(function(e) { 
      e.preventDefault(); 
      redirectStore(jQuery(this).attr('id'),jQuery(this).find('h2').text());
    });
     
    //  If click stay on store, store cookie and close fancybox 
    jQuery('.store-select.current').click(function(e) { 
      e.preventDefault(); 
       Cookies.set('locationpopupcookie', 'AUD', { path: '/', expires: 365 });
      jQuery('.country .country_code').text('Australia/Intl');
      jQuery.magnificPopup.close(); 
    });
           
    jQuery('.store-dropdown.newstore').click(function(e) { 
      e.preventDefault(); 
      redirectStore(jQuery(this).attr('id'),jQuery(this).find('a span').text());
    });
     
    //  If click stay on store, store cookie and close fancybox 
    jQuery('.store-dropdown.current').click(function(e) { 
      e.preventDefault(); 
      redirectStore(jQuery(this).attr('id'),jQuery(this).find('a span').text());
    });
  
    var geocountry = '';
    var georegion = '';
    var regioncookie = Cookies.get('locationpopupcookie');
    var regioncookiename = getRegionName(regioncookie);
          
    // var regioncookie = 'AU';
    // var regioncookiename = 'AU';
      
    // In no region cookie 
    if (!regioncookie){ 
	    
      // Geolocation option 
   GeolizrAPI.addEventListener('geolizr.geoData', function(geoData) {
       geocountry = geoData.country.code;
        
        console.log('GeoCountry = ' + geocountry);

        var euList = ['AL','AD','AM','AT','BY','BE','BA','BG','CH','CY','CZ','DE','DK','EE','ES','FO','FI','FR','GB','GE','GI','GR','HU','HR','IE','IS','IT','LT','LU','LV','MC','MK','MT','NO','NL','PL','PT','RO','RU','SE','SI','SK','SM','TR','UA','VA'];   
        if( $.inArray(geocountry, euList) != -1){
          geocountry = 'EU';
          console.log('Europe country');
        }
     
      console.log(geocountry);

        // Set location cookie based on loaction and updated select box, redirect if not current store
        if(geocountry == 'EU'){
       		georegion = 'EUR';
        }
       	else{
         	georegion = 'AUD';
       	}
     
        Cookies.set('locationpopupcookie', georegion, { path: '/', expires: 365 }); 
        regioncookie = Cookies.get('locationpopupcookie');
        jQuery('.country .country_code').text(getRegionName(regioncookie));

        // If geolocation does not meet store show popup
        if(regioncookie !== 'AUD'){
           console.log('Region does not meet current store ' + regioncookie + ' AUD');
           
           redirectStore(regioncookie,getRegionName(regioncookie));
      	} 
    }); // end of Geolizer event listener 
      
    }
    else{
     // If region set update dropdown
      if(regioncookiename != ''){
        console.log(regioncookiename);
        jQuery('.country .country_code').text(regioncookiename);
      }
    
       
    // If location cookie is different to region name
    if(regioncookie !== 'AUD'){
      
      // if any of the shared section
      if(jQuery('body').hasClass('template-page') || jQuery('body').hasClass('template-')
       || jQuery('body').hasClass('template-blog') || jQuery('body').hasClass('template-article')){
     // && !jQuery('body').hasClass('instagram')){
        
        // if from different region update navigation links
          
              var currentRegionURL = getRegionURL('AUD');
      
                 if(regioncookie == 'AUD'){
                 	var newRegionURL = '://www.thecriticalslidesociety.com';
                 }
                 else if(regioncookie == 'EUR'){
                 	var newRegionURL = '://www.thecriticalslidesociety.eu'; 
                 }
              
               console.log(newRegionURL);
             
          if(regioncookie == 'EUR'){
             jQuery('.country_wrapper .country_code').text('â¬' + regioncookie);
          }
           else {
             jQuery('.country_wrapper .country_code').text('$' + regioncookie);
           }
             
          $(".main-nav a[href^='/collections']").each(function(){ 
           	this.href = this.href.replace(currentRegionURL,newRegionURL);
          });
          
          $('a.account-link, a.current-cart').each(function(){
              this.href = this.href.replace(currentRegionURL,newRegionURL);
          });
       
      }
      // if not on a shared page then redirect     
      else{
        redirectStore(regioncookie,regioncookiename);
      }
      }  
  }

 });

</script>

